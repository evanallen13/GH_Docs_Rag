{
    "name": "Gh_Docs_Rag",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/Dev-Container-Compose",
    "postStartCommand": "pip install -r requirements.txt && alias py=python",
    "forwardPorts": [
        8080
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "ms-vscode.azurecli"
            ]
        }
    }
}